{
  "name": "prototype",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/prototype/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
